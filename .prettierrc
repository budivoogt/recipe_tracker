{
	"useTabs": true,
	"singleQuote": false,
	"semi": false,
	"trailingComma": "none",
	"tabWidth": 4,
	"printWidth": 100,
	"bracketSameLine": false,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"tailwindAttributes": ["searchClass", "innerDivClass", "divClass"]
}
